generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/prisma/notification"
}

datasource db {
  provider = "postgresql"
  url      = env("NOTIFICATION_DATABASE_URL")
}

model Notification {
  id      String  @id @db.Uuid
  userId  String  @db.Uuid
  content String  @db.VarChar(100)
  url     String  @db.VarChar(300)
  checked Boolean
}
